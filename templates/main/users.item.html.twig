<tr id="user_{{ users.id }}" data-user-id="{{ users.id }}">
    <td>{{ users.id }}</td>
    <td>{{ users.email }}</td>
    <td>{{ users.password }}</td>
    <td>{{ users.createdAt|format_datetime('long', 'medium', '') }}</td>

    {% if users.lastdateAt > 0 %}
        <td>{{ users.lastdateAt|format_datetime('long', 'medium', '') }}</td>
    {% else %}
        <td>Нет активности</td>
    {% endif %}
    <td>
                                    <span class="text-muted cursor-pointer" data-bs-toggle="tooltip"
                                          data-bs-placement="bottom" data-bs-original-title="Удалить"
                                          id="delete-users">
                                            <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24"
                                                 viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                                 stroke-linecap="round" stroke-linejoin="round"><path stroke="none"
                                                                                                      d="M0 0h24v24H0z"
                                                                                                      fill="none"></path><line
                                                        x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18"
                                                                                                   y2="18"></line></svg> </span>
    </td>
</tr>
